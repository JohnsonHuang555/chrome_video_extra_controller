{
  "name": "Video extra controller",
  "description": "Additional controls for any video, including playback speed and playback time.",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-32.png",
    "128": "images/icon-32.png"
  },
  "permissions": ["activeTab", "scripting"]
}
